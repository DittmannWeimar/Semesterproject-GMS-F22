System Test:

Master MasterName:
	bluetooth 'masterBluetooth'
	
	Slave plant01:
		bluetooth 'plant01'
		

			
		Actuator LED:
			pins (9)
			
		Actuator WaterPump:
			pins (10, 11, 12)
			Setting moistureLimit:
				default 10
				
		Sensor Moisture:
			pins (1, 2, 3, 4)
			graph moisture
			
		Sensor Temperature:
			pins (5, 6, 7, 8)
			graph temperature
				
	Slave plant02:
		bluetooth 'plant02'
		
		Actuator LED:
			pins (1)
			
		Actuator LED:
			pins (2)

Graph temperature:
	type Line
	category 'Greenhouse 01'
	title 'Temperature'
	xlabel 'time'
	ylabel 'temperature (C)'
	
